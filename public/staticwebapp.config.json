{
  "navigationFallback": {
    "rewrite": "/view/index.html",
    "exclude": ["/_astro/*", "/assets/*", "/potree/*", "*.{css,js,jpg,png,gif,ico,svg,woff,woff2,ttf,eot,json,wasm}"]
  },
  "routes": [
    {
      "route": "/view/*",
      "rewrite": "/view/index.html"
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  }
}
